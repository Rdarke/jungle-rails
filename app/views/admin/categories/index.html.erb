<section class="admin-categories-index">

  <header class="page-header">
    <h1>Categories</h1>
    
  </header>

  <div class="well">
    <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>

  <div class="panel panel-default categorys">

    <table class="table table-bordered">
      <thead>
        <tr>
          <th colspan="2">Catagory</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <%= render @categories %>
      </tbody>
    </table>

  </div>

</section>